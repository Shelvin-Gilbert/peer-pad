{
  "name": "peerpad",
  "version": "0.3.0",
  "private": true,
  "leadMaintainer": "Jacob Heun <jacobheun@gmail.com>",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "PUBLIC_URL='./' node scripts/build.js",
    "lint": "standard 'src/**/*.js' 'test/**/*.js'",
    "test": "jest --no-cache --env=jsdom",
    "test:e2e": "jest --testMatch **/test/e2e/**/*.js",
    "test:e2e:ci": "NO_SANDBOX=true URL=http://localhost:1337 run-p --race test:serve test:e2e",
    "test:e2e:load": "node test/e2e-load",
    "test:serve": "http-server -s -p 1337 build",
    "serve:build": "http-server -p 1337 build",
    "deploy:ipfs": "ipfs add -r build/",
    "start:rendezvous": "rendezvous"
  },
  "dependencies": {
    "autoprefixer": "9.3.1",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-preset-react-app": "^5.0.4",
    "babel-runtime": "6.26.0",
    "bs58": "^4.0.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.30.0",
    "color-hash": "^1.0.3",
    "css-loader": "1.0.0",
    "dotenv": "6.1.0",
    "eslint": "5.8.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "3.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "fast-diff": "^1.1.2",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "4.0.0-beta.2",
    "jest": "23.6.0",
    "katex": "^0.10.0-beta",
    "libp2p-crypto": "^0.14.0",
    "lodash.debounce": "^4.0.8",
    "milliseconds": "^1.0.3",
    "object-assign": "4.1.1",
    "peer-star-app": "^0.9.0",
    "pify": "^4.0.1",
    "postcss-css-variables": "^0.11.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "prop-types": "^15.6.0",
    "quill": "^1.3.2",
    "quill-delta-to-html": "^0.10.5",
    "react": "^16.6.0",
    "react-dev-utils": "^6.0.5",
    "react-dom": "^16.6.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.1.1",
    "remark": "^10.0.0",
    "remark-html": "^8.0.0",
    "remark-html-katex": "^1.1.1",
    "remark-math": "^1.0.4",
    "style-loader": "0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "tachyons": "^4.8.1",
    "tachyons-flexbox": "^2.0.5",
    "tachyons-forms": "^3.1.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.23.1",
    "webpack-dev-server": "3.1.10",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "delay": "^4.1.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "enzyme-to-json": "^3.3.4",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "libp2p-websocket-star-rendezvous": "^0.2.4",
    "npm-run-all": "^4.1.2",
    "puppeteer": "^1.9.0",
    "puppeteer-cluster": "^0.11.2",
    "react-test-renderer": "^16.6.0",
    "standard": "^12.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/enzyme.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+(?!\\.module\\.)\\.(css|styl)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|styl)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "homepage": "./"
}
